generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id            String   @id @default(cuid())
  question      String
  answers       String[]
  correctAnswer Int
  score         Int      @default(0)
  isWrong       Boolean  @default(false)
  category      String
}
